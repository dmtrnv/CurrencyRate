@model IEnumerable<MoneyModel>

@{
    ViewData["Title"] = "Курс валют";
}

<h4>Курс валют согласно ЦБ РФ - @ViewData["Date"]</h4><br />

<table width="100%">
    <tr>
        <th>Цифр. код</th>
        <th>Букв. код</th>
        <th>Единиц</th>
        <th>Валюта</th>
        <th>Курс</th>
    </tr>
    @foreach(var moneyModel in Model)
    {
    <tr>
        <td>@moneyModel.NumCode</td>
        <td>@moneyModel.CharCode</td>
        <td>@moneyModel.Nominal</td>
        <td>@moneyModel.Name</td>
        <td>@moneyModel.Value</td>
    </tr>
    }
</table>